{
  "name": "picture-app",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main",
  "type": "module",
  "scripts": {
    "dev": "DEV_ENV=1 concurrently \"npm run tsc:watch\" \"npm run electron\"",
    "dev:electron": "DEV_ENV=1 concurrently \"npm run tsc:watch\" \"npm run electron\"",
    "dev:electronmon": "DEV_ENV=1 concurrently \"npm run tsc:watch\" \"npm run electronmon\"",
    "electronmon": "ELECTRON_DISABLE_SANDBOX=1 electronmon dist/main.js",
    "next": "next dev --turbopack",
    "electron": "npm run tsc && ELECTRON_DISABLE_SANDBOX=1 electron dist/main.js",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "tsc": "tsc",
    "tsc:watch": "tsc -w -p tsconfig.tsc.json"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "concurrently": "^9.2.1",
    "express": "^5.1.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/electron": "^1.6.12",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "electron": "^37.4.0",
    "electron-rebuild": "^3.2.9",
    "electronmon": "^2.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4"
  }
}
